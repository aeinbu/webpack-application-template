{
  "name": "webpack-application-template",
  "version": "1.1.0",
  "description": "Framework neutral template for apps built with Webpack and Babel",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && webpack --config webpack.build.config.js",
    "build-release": "npm run clean && webpack -p --config webpack.build.config.js",
    "watch": "webpack --config webpack.build.config.js --watch",
    "start": "webpack-dev-server --config webpack.serve.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aeinbu/npm-package-for-ng-template.git"
  },
  "author": "Arjan Einbu <arjan@einbu.no> (http://blog.einbu.no)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "css-loader": "^0.26.1",
    "deepmerge": "^1.3.2",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0"
  }
}
